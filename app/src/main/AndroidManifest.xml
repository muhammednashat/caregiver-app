<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <uses-permission android:name="android.permission.SCHEDULE_EXACT_ALARM" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:name=".util.MyApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyProject"
        tools:targetApi="31">
        <activity
            android:name=".users.caregiver.tools.cofe.presintaion.SupportCofeActivity"
            android:exported="false" />
        <activity
            android:name=".users.patient.tools.coffeeideas.presentaion.CofeActivity"
            android:exported="false" />
        <activity
            android:name=".users.patient.moodTracking.presentaion.PostMoodTrackingActivity"
            android:exported="false" />
        <activity
            android:name=".users.patient.moodTracking.presentaion.MoodTrackingActivity"
            android:exported="false" />
        <activity
            android:name=".users.patient.calender.presentaion.CalenderActivity"
            android:exported="false" />
        <activity
            android:name=".commonFeatures.chatting.ChatActivity"
            android:exported="false" />
        <activity
            android:name=".commonFeatures.getLibraryContent.presentaion.LibraryActivity"
            android:exported="false" />
        <activity
            android:name=".maybeneedlater.TasksActivity"
            android:exported="false" />
        <activity
            android:name=".users.patient.dailyprogram.presentaion.DailyProgramActivity"
            android:exported="false" />
        <activity
            android:name=".users.patient.supporters.SupportersActivity"
            android:exported="false" />
        <activity
            android:name=".users.patient.profile.presentation.EditProfileActivity"
            android:exported="false" />
        <activity
            android:name=".util.TermsPrivacyActivity"
            android:exported="false" />
        <activity
            android:name=".users.patient.main.presentaion.UserScreensActivity"
            android:exported="false" />
        <activity
            android:name=".users.caregiver.main.CaregiverScreenActivity"
            android:exported="false" />
        <activity
            android:name=".auth.presentation.AuthActivity"
            android:exported="false" />
        <activity
            android:name=".users.patient.intro.IntroActivity"
            android:exported="false" />
        <activity
            android:name=".util.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_channel_id"
            android:value="encouragementChannelId" />

        <service
            android:name=".firebase.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <receiver
            android:name=".util.AlarmReceiver"
            android:exported="false" />
        <receiver
            android:name=".util.BootReceiver"
            android:exported="false" />
    </application>

</manifest>