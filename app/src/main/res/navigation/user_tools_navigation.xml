<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/user_tools_navigation.xml"
    app:startDestination="@id/toolsFragment">

    <fragment
        android:id="@+id/mainBreathFragment"
        android:name="mnshat.dev.myproject.users.patient.tools.breath.MainBreathFragment"
        android:label="fragment_main_breath"
        tools:layout="@layout/fragment_main_breath" />
    <fragment
        android:id="@+id/addSupplicationsFragment"
        android:name="mnshat.dev.myproject.users.patient.tools.supplications.prisentation.AddSupplicationsFragment"
        android:label="AddSupplicationsFragment" />
    <fragment
        android:id="@+id/mainSupplicationsFragment3"
        android:name="mnshat.dev.myproject.users.patient.tools.supplications.prisentation.MainSupplicationsFragment"
        android:label="MainSupplicationsFragment"
        tools:layout="@layout/fragment_main_supplications">
        <action
            android:id="@+id/action_mainSupplicationsFragment_to_supplicationsFragment"
            app:destination="@id/supplicationsFragment" />

        <action
            android:id="@+id/action_mainSupplicationsFragment_to_userSupplicationsFragment"
            app:destination="@id/userSupplicationsFragment" />
        <action
            android:id="@+id/action_mainSupplicationsFragment_to_addSupplicationsFragment"
            app:destination="@id/addSupplicationsFragment" />
    </fragment>
    <fragment
        android:id="@+id/userSupplicationsFragment"
        android:name="mnshat.dev.myproject.users.patient.tools.supplications.prisentation.UserSupplicationsFragment"
        android:label="UserSupplicationsFragment"
        tools:layout="@layout/fragment_user_supplications">

        <argument
            android:name="supplications"
            app:argType="mnshat.dev.myproject.model.Supplication[]" />
        <action
            android:id="@+id/action_userSupplicationsFragment_to_addSupplicationsFragment"
            app:destination="@id/addSupplicationsFragment" />
    </fragment>
    <fragment
        android:id="@+id/supplicationsFragment"
        android:name="mnshat.dev.myproject.users.patient.tools.supplications.prisentation.SupplicationsFragment"
        android:label="SupplicationsFragment"
        tools:layout="@layout/fragment_supplications">
        <argument
            android:name="supplication"
            app:argType="mnshat.dev.myproject.model.Supplication" />
    </fragment>
    <fragment
        android:id="@+id/gratitudeFragment"
        android:name="mnshat.dev.myproject.users.patient.tools.gratitude.GratitudeFragment"
        android:label="GratitudeFragment"
        tools:layout="@layout/fragment_gratitude" />
    <fragment
        android:id="@+id/toolsFragment"
        tools:layout="@layout/fragment_tools"
        android:name="mnshat.dev.myproject.users.patient.tools.ToolsFragment"
        android:label="ToolsFragment" >
        <action
            android:id="@+id/action_toolsFragment_to_gratitudeFragment2"
            app:destination="@id/gratitudeFragment" />
        <action
            android:id="@+id/action_toolsFragment_to_mainSupplicationsFragment3"
            app:destination="@id/mainSupplicationsFragment3" />
        <action
            android:id="@+id/action_toolsFragment_to_mainBreathFragment2"
            app:destination="@id/mainBreathFragment" />
    </fragment>
</navigation>