<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/patientHomeFragment">
    <fragment
        android:id="@+id/patientHomeFragment"
        android:name="mnshat.dev.myproject.users.patient.main.HomeFragment"
        android:label="fragment_patient_home"
        tools:layout="@layout/fragment_user_home" >
        <action
            android:id="@+id/action_patientHomeFragment_to_mainAzcarFragment"
            app:destination="@id/toolsFragment" />
        <action
            android:id="@+id/action_patientHomeFragment_to_educationalContentFragment"
            app:destination="@id/educationalContentFragment" />
    </fragment>

    <fragment
        android:id="@+id/profileFragment"
        android:name="mnshat.dev.myproject.users.patient.main.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_gratitudeListFragment"
            app:destination="@id/gratitudeListFragment" />
    </fragment>


    <fragment
        android:id="@+id/messagesFragment"
        android:name="mnshat.dev.myproject.users.patient.main.MessagesFragment"
        android:label="fragment_messages"
        tools:layout="@layout/fragment_messages" />
    <fragment
        android:id="@+id/gratitudeListFragment"
       tools:layout="@layout/fragment_gratitude_list"
        android:name="mnshat.dev.myproject.users.patient.main.GratitudeListFragment"
        android:label="GratitudeListFragment"
        app:popUpTo="@id/profileFragment"
        app:popUpToInclusive="false" >
        <action
            android:id="@+id/action_gratitudeListFragment_to_gratitudeFragment"
            app:destination="@id/gratitudeFragment" />
    </fragment>
    <fragment
        android:id="@+id/toolsFragment"
        tools:layout="@layout/fragment_tools"
        android:name="mnshat.dev.myproject.users.patient.main.tools.ToolsFragment"
        android:label="ToolsFragment">
        <action
            android:id="@+id/action_toolsFragment_to_mainSupplicationsFragment"
            app:destination="@id/mainSupplicationsFragment" />
        <action
            android:id="@+id/action_toolsFragment_to_mainBreathFragment"
            app:destination="@id/mainBreathFragment" />
        <action
            android:id="@+id/action_toolsFragment_to_gratitudeFragment"
            app:destination="@id/gratitudeFragment" />
    </fragment>

    <fragment
        android:id="@+id/mainSupplicationsFragment"
        tools:layout="@layout/fragment_main_supplications"
        android:name="mnshat.dev.myproject.users.patient.main.tools.supplications.MainSupplicationsFragment"
        android:label="MainSupplicationsFragment" >
        <action
            android:id="@+id/action_mainSupplicationsFragment_to_supplicationsFragment"
            app:destination="@id/supplicationsFragment" />

        <action
            android:id="@+id/action_mainSupplicationsFragment_to_userSupplicationsFragment"
            app:destination="@id/userSupplicationsFragment" />
    </fragment>

    <fragment
        android:id="@+id/supplicationsFragment"
        tools:layout="@layout/fragment_supplications"
        android:name="mnshat.dev.myproject.users.patient.main.tools.supplications.SupplicationsFragment"
        android:label="SupplicationsFragment" >
        <argument
            android:name="supplication"
            app:argType="mnshat.dev.myproject.model.Supplication" />
    </fragment>

    <fragment
        android:id="@+id/userSupplicationsFragment"
         tools:layout="@layout/fragment_user_supplications"
        android:name="mnshat.dev.myproject.users.patient.main.tools.supplications.UserSupplicationsFragment"
        android:label="UserSupplicationsFragment" >

        <argument
            android:name="supplications"
            app:argType="mnshat.dev.myproject.model.Supplication[]" />
    </fragment>
    <fragment
        android:id="@+id/mainBreathFragment"
        android:name="mnshat.dev.myproject.users.patient.main.tools.breath.MainBreathFragment"
        android:label="fragment_main_breath"
        tools:layout="@layout/fragment_main_breath" />
    <fragment
        android:id="@+id/gratitudeFragment"
        tools:layout="@layout/fragment_gratitude"
        android:name="mnshat.dev.myproject.users.patient.main.tools.gratitude.GratitudeFragment"
        android:label="GratitudeFragment" />
    <fragment
        android:id="@+id/educationalContentFragment"
        android:name="mnshat.dev.myproject.users.patient.libraraycontent.LibraryContentFragment"
        android:label="fragment_educational_content"
        tools:layout="@layout/fragment_educational_content" />
</navigation>