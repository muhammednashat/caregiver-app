<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/supportesFragment">

    <fragment
        android:id="@+id/supportesFragment"
        android:name="mnshat.dev.myproject.users.patient.supporters.presentation.SupportersFragment"
        android:label="fragment_supportes"
        tools:layout="@layout/fragment_supporters" >
        <action
            android:id="@+id/action_supportesFragment_to_addSupporterFragment"
            app:destination="@id/addSupporterFragment" />


        <action
            android:id="@+id/action_supportesFragment_to_supporterDetailsFragment"
            app:destination="@id/supporterDetailsFragment">


        </action>

    </fragment>


    <fragment
        android:id="@+id/addSupporterFragment"
      tools:layout="@layout/fragment_add_supporter"
        android:name="mnshat.dev.myproject.users.patient.supporters.presentation.AddSupporterFragment"
        android:label="AddSupporterFragment" >
        <action
            android:id="@+id/action_addSupporterFragment_to_createNewInvitationCodeFragment"
            app:destination="@id/createNewInvitationCodeFragment" />
    </fragment>
    <fragment
        tools:layout="@layout/fragment_supporter_details"
        android:id="@+id/supporterDetailsFragment"
        android:name="mnshat.dev.myproject.users.patient.supporters.presentation.SupporterDetailsFragment"
        android:label="SupporterDetailsFragment" >
        <argument
            android:name="supporter"
            app:argType="mnshat.dev.myproject.auth.data.entity.UserProfile" />

    </fragment>

    <fragment
        android:id="@+id/createNewInvitationCodeFragment"
        android:name="mnshat.dev.myproject.users.patient.supporters.presentation.CreateNewInvitationCodeFragment"
        android:label="CreateNewInvitationCodeFragment" />
</navigation>