<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/caregiverHomeFragment">
    <fragment
        android:id="@+id/caregiverHomeFragment"
        android:name="mnshat.dev.myproject.users.caregiver.main.CaregiverHomeFragment"
        android:label="fragment_caregiver_home"
        tools:layout="@layout/fragment_caregiver_home" >
        <action
            android:id="@+id/action_caregiverHomeFragment_to_usersFragment"
            app:destination="@id/usersFragment" />
    </fragment>
    <fragment
        android:id="@+id/caregiverProfileFragment"
        android:name="mnshat.dev.myproject.users.caregiver.main.CaregiverProfileFragment"
        android:label="fragment_caregiver_profile"
        tools:layout="@layout/fragment_caregiver_profile" />
    <fragment
        android:id="@+id/usersFragment"
        android:name="mnshat.dev.myproject.users.caregiver.partner.PatientFragment"
        android:label="UsersFragment" />

    <navigation
        android:id="@+id/chatting_navigation"
        app:startDestination="@id/chatting_navigation.xml">
        <include
            app:graph="@navigation/chatting_navigation" />
    </navigation>


</navigation>