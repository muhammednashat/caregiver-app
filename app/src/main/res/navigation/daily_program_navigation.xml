<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dialy_program_navigation.xml"
    app:startDestination="@id/educationalFragment">

    <fragment
        android:id="@+id/educationalFragment"
        android:name="mnshat.dev.myproject.users.patient.dailyprogram.EducationalFragment"
        android:label="EducationalFragment">
        <action
            android:id="@+id/action_educationalFragment_to_spiritualFragment"
            app:destination="@id/spiritualFragment" />
        <action
            android:id="@+id/action_educationFragment_to_behaviorActivationFragment"
            app:destination="@id/behaviorActivationFragment" />
        <action
            android:id="@+id/action_educationalFragment_to_preMoodSelectionFragment"
            app:destination="@id/preMoodSelectionFragment"
            app:popUpTo="@id/educationalFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/behaviorActivationFragment"
        android:name="mnshat.dev.myproject.users.patient.dailyprogram.BehaviouralActivationFragment"
        android:label="ActivityFragment" >
        <action
            android:id="@+id/action_behaviorActivationFragment_to_congratulationsFragment"
            app:destination="@id/congratulationsFragment" />
    </fragment>



    <fragment
        android:id="@+id/spiritualFragment"
        android:name="mnshat.dev.myproject.users.patient.dailyprogram.SpiritualFragment"
        android:label="BehaviorFragment" >
        <action
            android:id="@+id/action_spiritualFragment_to_activityFragment"
            app:destination="@id/behaviorActivationFragment" />
    </fragment>



    <fragment
        android:id="@+id/congratulationsFragment"
        android:name="mnshat.dev.myproject.users.patient.dailyprogram.CongratulationsFragment"
        android:label="CongratulationsFragment" />
    <fragment
        android:id="@+id/preMoodSelectionFragment"
        android:name="mnshat.dev.myproject.users.patient.moodTracking.presentaion.PreMoodSelectionFragment"
        android:label="PreMoodSelectionFragment" >
        <action
            android:id="@+id/action_preMoodSelectionFragment_to_educationalFragment"
            app:destination="@id/educationalFragment"
            app:popUpTo="@id/preMoodSelectionFragment"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>