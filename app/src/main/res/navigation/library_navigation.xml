<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/library_navigation.xml"
    app:startDestination="@id/libraryContentFragment">

    <fragment
        android:id="@+id/libraryContentFragment"
        tools:layout="@layout/fragment_library_content"
        android:name="mnshat.dev.myproject.users.patient.libraraycontent.LibraryContentFragment"
        android:label="LibraryContentFragment" >
        <action
            android:id="@+id/action_libraryContentFragment_to_customizedContentFragment"
            app:destination="@id/customizedContentFragment" />
        <action
            android:id="@+id/action_libraryContentFragment_to_articleFragment"
            app:destination="@id/articleFragment" />
        <action
            android:id="@+id/action_libraryContentFragment_to_audioFragment"
            app:destination="@id/audioFragment" />
        <action
            android:id="@+id/action_libraryContentFragment_to_videoFragment"
            app:destination="@id/videoFragment" />
    </fragment>
    <fragment
        android:id="@+id/customizedContentFragment"
        tools:layout="@layout/fragment_customized_content"
        android:name="mnshat.dev.myproject.users.patient.libraraycontent.CustomizedContentFragment"
        android:label="CustomizedContentFragment" >

        <argument
            android:name="libraryContentList"
            app:argType="mnshat.dev.myproject.model.LibraryContent[]" />

        <argument
            android:name="textTitle"
            app:argType="string" />
        <action
            android:id="@+id/action_customizedContentFragment_to_articleFragment"
            app:destination="@id/articleFragment" />
        <action
            android:id="@+id/action_customizedContentFragment_to_audioFragment"
            app:destination="@id/audioFragment" />
        <action
            android:id="@+id/action_customizedContentFragment_to_videoFragment"
            app:destination="@id/videoFragment" />

    </fragment>
    <fragment
        android:id="@+id/articleFragment"
        tools:layout="@layout/fragment_article"
        android:name="mnshat.dev.myproject.users.patient.libraraycontent.ArticleFragment"
        android:label="ArticleFragment" />
    <fragment
        android:id="@+id/audioFragment"
        android:name="mnshat.dev.myproject.users.patient.libraraycontent.AudioFragment"
        android:label="fragment_audio"
        tools:layout="@layout/fragment_audio" />
    <fragment
        android:id="@+id/videoFragment"
        tools:layout="@layout/fragment_video"
        android:name="mnshat.dev.myproject.users.patient.libraraycontent.VideoFragment"
        android:label="VideoFragment" />
</navigation>